<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="Value_O"
    targetNamespace="http://tempuri.org/Value_O.xsd"
    elementFormDefault="qualified"
    xmlns="http://tempwuri.org/Value_O.xsd"
    xmlns:mstns="http://tempuri.org/Value_O.xsd"
